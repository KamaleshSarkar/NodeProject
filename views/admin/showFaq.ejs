<%- include('layouts/header')%>
<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <%- include('layouts/sidebar')%>
        
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">
            
            <!-- Main Content -->
            <div id="content">
        <%- include('layouts/topnav')%>


                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
               
                   

                    <!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <div class="card-header py-3">
                 <h4 class="m-0 font-weight-bold text-primary">User Listing</h4> <br>
                            <a  class="m-0 font-weight-bold text-primary" href="/questionFAQ">Add Qustion</a>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Serial No</th>
                                            <th>Question</th>
                                            <th>Answer</th>
                                            <th>Edit</th>
                                            <th>Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% userQuestion.map((data,index) =>{ %>
                                            <tr>
                                             
                                              <td><%=index+1%></td>
                                              <td><%=data.question%></td>
                                              <td><%=data.answer%></td>
                                              <td><a href="">Edit</a></td>
                                              <td><a href="/delete-Question/<%=data._id%>">Delete</a></td>
                                              </tr>
                                            <%})%>
                                    </tbody>
                                  
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->


<%- include('layouts/siteFooter')%>
<%- include('layouts/footer')%>


           